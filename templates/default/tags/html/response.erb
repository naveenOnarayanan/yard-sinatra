<% if object.has_tag?(:response) %>
  <div class="examples">
    <p class="tag_title">Response:</p>
    <% object.tags(:response).each do |tag| %>
      <%= htmlify_line(tag.name) %>
      <%= puts "Name: #{tag.name}"%>
      <pre class="example code source_code">
          <%= puts "text: #{tag.text}"%>
          <%=html_syntax_highlight(tag.text, :plain) %>
      </pre>
    <% end %>
  </div>
<% end %>
